{
    "name": "statamic/seo-pro",
    "autoload": {
        "psr-4": {
            "Statamic\\SeoPro\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Seo Pro",
            "description": "Seo Pro addon"
        },
        "laravel": {
            "providers": [
                "Statamic\\SeoPro\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "4.0.0-beta.2 as 4.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.20.0 || ^6.22.0 || ^7.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
